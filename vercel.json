{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ],
    "crons": [
        {
            "path": "/api/v1/jetbus/cron",
            "schedule": "*/10 * * * *"
        }
    ]
}